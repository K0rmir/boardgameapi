{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.ts"
      }
    ], 
    "crons": [
      {
      "path": "/lib/cron",
      "schedule": "*/1 * * * *"
      }
    ]
  }
  