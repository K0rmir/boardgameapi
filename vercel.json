{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.ts"
      }
    ],
    "crons": [
      {
      "path": "src/lib/cron",
      "schedule": "0 0 * * *"
      }
    ]
  }
  